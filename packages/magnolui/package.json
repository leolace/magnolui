{
  "name": "@magnolui/react",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.tsx",
  "scripts": {
    "typecheck": "tsc -b --noEmit",
    "build:tsc": "tsc -b tsconfig.build.json",
